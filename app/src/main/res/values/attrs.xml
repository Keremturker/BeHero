<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEdittext">
        <attr name="leftImage" format="integer" />
        <attr name="android:inputType" />
        <attr name="android:digits" />
        <attr name="android:hint" />
        <attr name="android:background" />
        <attr name="android:imeOptions" />
        <attr name="android:maxLength" />
        <attr name="android:singleLine" />
        <attr name="android:enabled" />
        <attr name="android:fontFamily" />
        <attr name="android:minLines" />
        <attr name="android:maxLines" />
        <attr name="android:gravity" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="image" format="integer" />
        <attr name="text" format="string" />
        <attr name="backgroundColor" format="color|reference" />
        <attr name="android:enabled" />
        <attr name="android:fontFamily" />
    </declare-styleable>


    <declare-styleable name="multi_line_radio_group">
        <attr name="max_in_row" format="integer" />
        <attr name="radio_buttons" format="reference" />
        <attr name="default_button" format="string" />
        <attr name="tintColor" format="color" />
    </declare-styleable>
</resources>